{
  "files.autoSave": "onFocusChange",
  // breadcrumbs
  "breadcrumbs.enabled": true,
  // editor
  "editor.renderLineHighlight": "line",
  "editor.gotoLocation.multipleLocations": ["goto"],
  "editor.semanticHighlighting.enabled": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": false
  },
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#212121"
  },
  "editor.hideCursorInOverviewRuler": true,
  "editor.scrollBeyondLastLine": false,
  "editor.lineHeight": 22,
  "editor.occurrencesHighlight": false,
  "editor.multiCursorModifier": "alt",
  "editor.overviewRulerBorder": false,
  "editor.overviewRulerLanes": 0,
  "editor.renderIndentGuides": false,
  "editor.folding": false,
  "editor.snippetSuggestions": "top",
  "editor.lightbulb.enabled": false,
  "editor.minimap.enabled": false,
  "editor.cursorBlinking": "solid",
  "editor.lineNumbers": "on",
  "editor.cursorStyle": "line",
  // window
  "window.zoomLevel": -1,
  "window.restoreWindows": "all",
  "window.newWindowDimensions": "maximized",
  // debug
  "debug.openDebug": "neverOpen",
  "debug.showInStatusBar": "never",
  "debug.toolBarLocation": "hidden",
  // fonts
  "editor.fontFamily": "Fira Code",
  "terminal.integrated.fontSize": 13,
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  // search
  "search.smartCase": true,
  "search.exclude": {
    "dist": true,
    "out": true,
    "coverage": true,
    ".history": true,
    ".cache": true
  },
  "eslint.workingDirectories": ["packages/*"],
  "eslint.workingDirectories": ["packages/*"],
  // terminal
  "terminal.integrated.copyOnSelection": true,
  // files
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/package-lock.json": true,
    "vendor": true,
    "**/node_modules": true,
    "**/npm-debug.log": true,
    "**/dist": true,
    "**/*.bs.js": true
  },
  "editor.formatOnSave": true,
  // code runner
  "code-runner.runInTerminal": false,
  // vim

  "vim.disableExtension": true,
  "vim.leader": " ",
  "vim.debug.silent": true,
  "vim.cursorStylePerMode.normal": "underline",
  "vim.useSystemClipboard": true,
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "red",
  "explorer.confirmDelete": false,
  // python
  "python.pythonPath": "/run/current-system/sw/bin/python3",
  // TODO: maybe there is better? It also doesn't work
  "python.formatting.provider": "autopep8",
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": false,
  // TODO: maybe not need?
  // typescript
  "typescript.check.npmIsInstalled": false,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "typescript.suggest.autoImports": true,
  "typescript.format.enable": true,
  // javascript
  "javascript.referencesCodeLens.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  // git
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.ignoreLimitWarning": true,
  "git.enableSmartCommit": true,
  "git.ignoreMissingGitWarning": true,
  // gitlens
  "gitlens.codeLens.enabled": false,
  "gitlens.settings.mode": "advanced",
  "gitlens.keymap": "chorded",
  "gitlens.codeLens.scopes": ["containers", "document"],
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "gitlens.advanced.messages": {
    "suppressFileNotUnderSourceControlWarning": true,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": true
  },
  // Markdown
  "markdown-pdf.executablePath": "/Applications/Chromium.app/Contents/MacOS/Chromium",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "aws.samcli.location": "/usr/local/bin/sam",
  "aws.profile": "profile:guide-deployer",
  "aws.onDefaultRegionMissing": "add",
  "tslint.packageManager": "yarn",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "dart.openDevTools": "flutter",
  "workbench.colorTheme": "Monokai Night",
  "todo-tree.tree.showScanModeButton": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.wordWrap": "on",
  "[scss]": {},
  "vscode-neovim.highlightGroups.unknownHighlight": {},
  "vscode-neovim.neovimPath": "/usr/local/bin/nvim",
  "vim.easymotionMarkerFontSize": "",
  "go.useLanguageServer": true,
  "go.languageServerExperimentalFeatures": {
    "diagnostics": true,
    "documentLink": true
  },
  "go.gopath": "~/go",
  "[go]": {
    "editor.snippetSuggestions": "none",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  }
}
